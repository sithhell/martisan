{
    "name": "gmp",
    "category": "Math Library",
    "url": "https://gmplib.org/",
    "description": "The GNU Multiple Precision Arithmetic Library",
    "versions":
        [
            ["6.1.1", "https://gmplib.org/download/gmp/gmp-6.1.1.tar.bz2"]
        ],
    "build":
        [
            "cd $BUILD_DIR",
            "$SRC_DIR0/configure --prefix=$PACKAGE_PREFIX",
            "make -j8",
            "make install"
        ],
    "architectures": "all"
}
